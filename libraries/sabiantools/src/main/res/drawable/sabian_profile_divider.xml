<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/sabian_profile_divider"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>

    <item android:top="1dp" android:right="1dp" android:left="0dp" android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/sabian_profile_divider"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</layer-list>