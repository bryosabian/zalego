<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#cc3A505B" />
            <stroke android:width="1dp" android:color="#cc3A505B" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/bbutton_camouflage_edge_transparent" />
            <stroke android:width="1dp" android:color="@color/bbutton_camouflage_edge_transparent" />
        </shape>
    </item>


    <item>
        <shape>
            <solid android:color="@color/bbutton_camouflage_transparent" />
            <stroke android:width="1dp" android:color="@color/bbutton_camouflage_transparent" />
        </shape>
    </item>

</selector>